        "@types/express": "^4.17.7",
        "better-sqlite3": "^7.1.0",
        "better-sqlite3-helper": "^3.1.0",
        "cpy-cli": "^3.1.1",
        "crypto-js": "^4.0.0",
        "http-signature": "^1.3.4",
        "http-status-codes": "^1.4.0",
        "turtlecoin-utils": "^2.0.2"
    },
    "devDependencies": {
        "@types/node": "^14.0.26",
        "nodemon": "^2.0.20",
        "rimraf": "^3.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
