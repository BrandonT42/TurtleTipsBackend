{
    "name": "turtletips_backend",
    "version": "0.0.1",
    "description": "Backend for TurtleTips wallet service",
    "repository": {
        "type": "git",
        "url": "git://github.com/BrandonT42/TurtleTipsBackend.git"
    },
    "main": "index.ts",
    "scripts": {
        "clean": "rm -rf \"dist\" || true; cpy \"src/migrations/*\" \"dist/migrations\"",
